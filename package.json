{
  "scripts": {
    "dev": " concurrently \"npm run snapp\" \"npm run tailwindcss\" \"npm run serve\" ",
    "serve": "npx live-server public",
    "snapp": "snapp build -s public/views -o public/src -W",
    "tailwindcss": "npx @tailwindcss/cli -i ./public/style/input.css -o ./public/style/output.css --watch"
  },
  "devDependencies": {
    "@fortawesome/free-brands-svg-icons": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@tailwindcss/cli": "^4.1.15",
    "concurrently": "^9.2.1",
    "live-server": "^1.2.2",
    "snapp-icons": "^1.0.1",
    "tailwindcss": "^4.1.15"
  }
}
